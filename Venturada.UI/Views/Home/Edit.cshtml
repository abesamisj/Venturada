@model Venturada.UI.ViewModels.HomeViewModels
@{
    ViewBag.Title = "Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@if (Session["UserName"]  != null)
{
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Edit Home <small>add images? edit main paragraph? edit your featured sites?</small></h1>
                <ol class="breadcrumb">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>Edit Home</li>
                </ol>
            </div>
        </div><!-- /.row -->
        @Html.Partial("_EditCarouselsPartialPage", Model.CarouselsViewModels)
    </div><!-- /.container-->
    <div class="section-colored">
        <div class="container">

            @Html.Partial("_EditMainPartial", Model.MainViewModels)
        </div>
    </div>
    <div class="section">
        <div class="container">
            @Html.Partial("_EditFeatureMainPartial", Model.FeatureMainModel)
        </div>
    </div>
}
else
{
    @Html.Partial("_NoAccess")
}
